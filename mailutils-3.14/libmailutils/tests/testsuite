liber:2.1-3: two\\n
liber:3.1-5: three
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/linetrack.at:96"
$at_failed && at_fn_log_failure
$at_traceon; }

