  "Url: ftp://user:pass@host/a/path" "               " 13
at_xfail=no
(
  $as_echo "343. $at_setup_line: testing $at_desc ..."
  $at_traceon


{ set +x
$as_echo "$at_srcdir/url.at:219:
cat >input <<'_ATEOF'
ftp://user:pass@host/a/path
_ATEOF

url-parse default < input"
